<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <DisableWinExeOutputInference>true</DisableWinExeOutputInference>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>false</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommunityToolkit.Mvvm" Version="8.4.0" />
    <PackageReference Include="DevExpress.Mvvm" Version="24.2.3" />
    <PackageReference Include="DevExpress.Wpf" Version="24.2.3" />
    <PackageReference Include="DevExpress.Wpf.Themes.VS2019Blue" Version="24.2.3" />
    <PackageReference Include="DevExpress.Wpf.Themes.VS2019Dark" Version="24.2.3" />
    <PackageReference Include="DevExpress.Wpf.Themes.VS2019Light" Version="24.2.3" />
    <PackageReference Include="DevExpress.Wpf.Themes.Win11Dark" Version="24.2.3" />
    <PackageReference Include="DevExpress.Wpf.Themes.Win11Light" Version="24.2.3" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.77" />
  </ItemGroup>

</Project>
